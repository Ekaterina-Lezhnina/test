class Goose:
    def __init__(self, name, weight, sound):
        self.name = name
        self.weight = weight
        self.sound = sound

    def feed(self):
        print(self.name, 'Накормлен')

    def cut(self):
        print(self.name, 'Подстрижен')


grey_goose = Goose('Серый', 5, 'га-га-га')
white_goose = Goose('Белый', 5, 'га-га-га')
print(white_goose.feed())
print(grey_goose.cut())

